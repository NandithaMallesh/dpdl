<h1>New Patient</h1>

<%= form_tag({:action => 'create', :patient => @patient}, :multipart => true) do %>

  <div class="field">
    <%= label_tag :file %>
    <%= file_field_tag :file,  accept: '.json', :required => true%>
  </div>
  <div class="actions">
    <%= submit_tag "Upload" %>
  </div>
<% end %>

<%= link_to 'Back', patients_path %>
